{
  "version": 2,
  "builds": [
    {
      "src": "Backend/App.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./Backend/App.js",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
        
      }
    }
  ]
}